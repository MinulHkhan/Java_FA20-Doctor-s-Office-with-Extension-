package com.company;

public class Main {

    public static void main(String[] args) {

        String X;
        X = "Patient Details:";
        String a;
        a = "Stacey Manson";
        Boolean b;
        b = false;
        float c;
        c = 102.2f;
        int d;
        d = 5;

        System.out.println(X);
        System.out.println("Name: " + a);
        System.out.println("Patient has insurance: " + b);
        System.out.println("Body temprature: " + c);
        System.out.println("Number of visits:" + d);

    }
}
